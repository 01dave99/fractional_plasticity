retrieve_ks<-function(dat)
{
ks<-rep(0,ncol(dat))
for (i in 1: ncol(dat))
{
for (j in 2: nrow(dat))
{
if (dat[j,i]==0)
{
ks[i]=j-2
break
}
}
}
return(ks)
}
