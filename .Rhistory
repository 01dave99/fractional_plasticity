data$step=0:(nrow(data)-1)
write.table(data,file = "frac_res_3398_0.5_adj.csv",sep = ",",na="",row.names = FALSE,col.names = TRUE,quote = FALSE)
#kplot for different parameters alpha
dat1<-read.csv("frac_res_3398_0.5.csv",header = F)
dat2<-read.csv("frac_res_3398_0.7.csv",header = F)
dat3<-read.csv("frac_res_3398_0.9.csv",header = F)
dat4<-read.csv("frac_res_3398_0.99.csv",header = F)
ks1<-retrieve_ks(dat1)
ks2<-retrieve_ks(dat2)
ks3<-retrieve_ks(dat3)
ks4<-retrieve_ks(dat4)
alpha_table=data.frame(ks=c(ks1,ks2,ks3,ks4),alpha=c(rep("0.5",ncol(dat1)),rep("0.7",ncol(dat1)),rep("0.9",ncol(dat1)),rep("0.99",ncol(dat1))),t=rep(1:ncol(dat1),4))
View(alpha_table)
tikz(file = "frac_ks_alpha.tex", width=5, height=2.5)
library(tikzDevice)
tikz(file = "frac_ks_alpha.tex", width=5, height=2.5)
ggplot(data=alpha_table)+geom_line(aes(x=t,y=ks,color=alpha),linewidth=1)+labs(x="$t_n$",color="$\\alpha$",y="$k$")+scale_color_manual(breaks = c("0.5","0.7","0.9","0.99"),values = c("#E69F00","#56B4E9", "#009E73", "#CC79A7"))+theme_minimal()+theme(axis.title.y = element_text(angle = 0, vjust = 0.5))
dev.off()
